# Iris Environment Configuration - Ultra Fast Agentic AI
# Copy this to .env and fill in your values

# Google Gemini API Configuration
GOOGLE_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_THINKING_BUDGET=medium

# Redis Configuration (for Dramatiq and caching)
REDIS_URL=redis://localhost:6379

# Tool Configuration
TOOL_CACHE_TTL=300
JOB_TTL=3600
EVENT_TTL=1800
MAX_EVENTS_PER_JOB=1000

# Performance Settings
STREAMING_ENABLED=true
PARALLEL_TOOLS=true
WEBSOCKET_ENABLED=true

# Optional: Supabase Configuration (for future use)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Optional: Other LLM APIs (for fallback)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Daytona Configuration (for sandbox execution)
DAYTONA_API_KEY=your_daytona_api_key_here

# Development Settings
DEBUG=true
LOG_LEVEL=info
